---
import Layout from '../layouts/Layout.astro';

const pageTitle = "Professional House Cleaning Knoxville | Hero";
const pageDescription = "Professional house cleaning in Knoxville with background-checked, trained teams. 4.8 stars from 175+ reviews. Insured, bonded, and professional-grade service. Call (865) 507-1405.";

// Structured Data - LocalBusiness Schema
